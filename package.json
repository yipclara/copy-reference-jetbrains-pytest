{
  "name": "copy-reference-jetbrains-pytest",
  "displayName": "Copy Reference Pytest",
  "description": "JetBrains-style copy reference for Python (pytest) in VS Code. Right-click a Python symbol to copy its pytest-style reference (module::Class::method).",
  "version": "1.0.0",
  "publisher": "yipclara",
  "repository": {
    "type": "git",
    "url": "https://github.com/yipclara/copy-reference-jetbrains-pytest.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Testing"
  ],
  "keywords": [
    "copy",
    "reference",
    "pytest",
    "pycharm",
    "python",
    "testing",
    "django",
    "unittest"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onCommand:copyReference.copyPath.pytest"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyReference.copyPath.pytest",
        "title": "Copy Reference - Pytest",
        "icon": "$(symbol-class)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "copyReference.copyPath.pytest",
          "group": "9_cutcopypaste@2",
          "when": "editorTextFocus && editorLangId == 'python'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --out releases/",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.5"
  },
  "author": "Clara Yip",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yipclara/copy-reference-jetbrains-pytest/issues"
  },
  "homepage": "https://github.com/yipclara/copy-reference-jetbrains-pytest#readme"
}
